{
    "name": "leetcode-query",
    "version": "0.2.4",
    "description": "Get user profiles, submissions, and problems on LeetCode.",
    "main": "lib/index.js",
    "module": "lib/index.mjs",
    "types": "lib/index.d.ts",
    "files": [
        "lib"
    ],
    "scripts": {
        "prepare": "npm run build && npm run docs",
        "dev": "tsup --watch",
        "build": "npm run format && tsup",
        "docs": "typedoc ./src/ --name \"LeetCode Query\" --customCss \"./docs.css\"",
        "format": "prettier --write \"src/**/*.ts\"",
        "lint": "eslint .",
        "test": "jest --coverage"
    },
    "keywords": [
        "leetcode",
        "api"
    ],
    "author": "JacobLinCool <jacoblincool@gmail.com> (https://github.com/JacobLinCool)",
    "license": "MIT",
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "3.2.0",
        "@types/jest": "27.5.1",
        "@types/node": "17.0.35",
        "@types/node-fetch": "2.6.1",
        "@typescript-eslint/eslint-plugin": "5.25.0",
        "@typescript-eslint/parser": "5.25.0",
        "dotenv": "16.0.1",
        "eslint": "8.18.0",
        "eslint-config-prettier": "8.5.0",
        "jest": "28.1.0",
        "prettier": "2.7.1",
        "ts-jest": "28.0.2",
        "tsup": "5.12.8",
        "typedoc": "0.22.18",
        "typescript": "4.6.4"
    },
    "dependencies": {
        "node-fetch": "2.6.7"
    },
    "bugs": {
        "url": "https://github.com/JacobLinCool/LeetCode-Query/issues"
    },
    "homepage": "https://jacoblincool.github.io/LeetCode-Query/",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/JacobLinCool/LeetCode-Query.git"
    }
}
